<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.niravtukadiya.compat.biometric.sample">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/nt_ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/nt_ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">


        <activity android:name="com.niravtukadiya.compat.biometric.sample.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <action android:name="android.intent.action.VIEW" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

    </application>
</manifest>
